{
	"name": "doma",
	"version": "3.0.1",
	"description": "Parse an HTML string into `DocumentFragment` or one `Element`, in a few bytes (in browser)",
	"keywords": [
		"browser",
		"dom",
		"domify",
		"innerhtml",
		"jquery",
		"outerhtml",
		"parse html",
		"string to dom",
		"vanilla"
	],
	"repository": "fregante/doma",
	"license": "MIT",
	"type": "module",
	"main": "index.js",
	"files": [
		"index.js",
		"index.d.ts"
	],
	"scripts": {
		"build": "tsc",
		"prepack": "tsc --sourceMap false",
		"test": "tsc && xo && ava",
		"watch": "tsc --watch"
	},
	"xo": {
		"envs": [
			"browser"
		]
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^3.0.1",
		"ava": "^4.3.1",
		"jsdom": "^20.0.0",
		"typescript": "^4.7.4",
		"xo": "^0.51.0"
	}
}
