{
	"name": "doma",
	"version": "4.0.0",
	"description": "Parse an HTML string into `DocumentFragment` or one `Element`, in a few bytes (in browser)",
	"keywords": [
		"browser",
		"dom",
		"domify",
		"innerhtml",
		"jquery",
		"outerhtml",
		"parse html",
		"string to dom",
		"vanilla"
	],
	"repository": "fregante/doma",
	"funding": "https://github.com/sponsors/fregante",
	"license": "MIT",
	"type": "module",
	"exports": "./index.js",
	"main": "./index.js",
	"types": "./index.d.ts",
	"files": [
		"index.js",
		"index.d.ts"
	],
	"scripts": {
		"build": "tsc",
		"prepack": "tsc --sourceMap false",
		"test": "tsc && xo && ava",
		"watch": "tsc --watch"
	},
	"xo": {
		"envs": [
			"browser"
		]
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^3.0.1",
		"ava": "^5.3.1",
		"jsdom": "^22.1.0",
		"typescript": "^5.1.6",
		"xo": "^0.54.2"
	},
	"engines": {
		"node": ">=18"
	}
}
